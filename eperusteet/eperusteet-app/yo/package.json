    "angular-local-storage": "0.7.1",
    "angular-moment": "1.0.0-beta.3",
    "angular-resource": "1.6.6",
    "angular-route": "1.6.6",
    "angular-sanitize": "1.6.6",
    "angular-spinner": "0.8.0",
    "angular-translate": "2.8.1",
    "angular-translate-loader-static-files": "2.15.2",
    "angular-ui-bootstrap": "2.5.0",
    "angular-ui-router": "0.4.2",
    "angular-ui-sortable": "https://github.com/nkala/ui-sortable.git#v0.13.4xx",
    "angular-ui-tree": "2.22.6",
    "angular-ui-utils": "0.1.1",
    "angularjs-slider": "5.8.7",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "ckeditor": "4.7.0",
